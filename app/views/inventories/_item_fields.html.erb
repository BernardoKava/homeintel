

<div class="nested-fields">
  <div class="field">
    <%= link_to_remove_association 'Remove', f, class: 'fa fa-close', style:"font-size:15px;color:red"%>
  </div>

  <div class="field">
    <%= f.label  "Name:"  %>
    <%= f.text_field :item, class: 'form-control'%>
  </div>

  <div class="field">
    <%= f.label  "Description:"  %>
    <%= f.text_area :description, class: 'form-control' %>
  </div>

  <div class="field">
    <%= f.label  "Quantity:"  %>
    <%= f.text_field :quantity , class: 'form-control'%>
  </div>

  <div class="field">
    <%= f.label  "Value:"  %>
    <%= f.text_field :value, class: 'form-control' %>
  </div>

  <div class="field">
    <%= f.label  "Owner:"  %>
    <%= f.text_field :owner, class: 'form-control' %>
  </div>

</div>

<hr/>
<hr/>




